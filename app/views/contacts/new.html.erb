<h1>Contact Us</h1>
<%= form_tag contacts_path do %>
<div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
</div>
<div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
</div>
<div>
    <%= label_tag :body %>
    <%= text_area_tag :body %>
</div>
<%= submit_tag "Submit" %>
<% end %>
